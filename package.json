{
    "name": "travelo",
    "version": "1.0.0",
    "scripts": {
      "start": "npm-run-all --parallel start:backend start:frontend",
      "start:backend": "cd backend && node App.js",
      "start:frontend": "cd frontend/my-app && npm start",
      "build": "cd frontend/my-app && npm run build",
      "install-all": "npm install --workspaces"
    },
    "devDependencies": {
      "npm-run-all": "^4.1.5"
    },
    "workspaces": [
      "backend",
      "frontend/my-app"
    ]
  }